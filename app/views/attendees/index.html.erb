<p id="notice"><%= notice %></p>

<h1>Listing Attendees</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Title</th>
      <th>Company</th>
      <th>Address 1</th>
      <th>Address 2</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>Country</th>
      <th>Direct phone</th>
      <th>Mobile phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attendees.each do |attendee| %>
      <tr>
        <td><%= attendee.first_name %></td>
        <td><%= attendee.last_name %></td>
        <td><%= attendee.title %></td>
        <td><%= attendee.company %></td>
        <td><%= attendee.address_1 %></td>
        <td><%= attendee.address_2 %></td>
        <td><%= attendee.city %></td>
        <td><%= attendee.state %></td>
        <td><%= attendee.zip_code %></td>
        <td><%= attendee.country %></td>
        <td><%= attendee.direct_phone %></td>
        <td><%= attendee.mobile_phone %></td>
        <td><%= attendee.email %></td>
        <td><%= link_to 'Show', attendee %></td>
        <td><%= link_to 'Edit', edit_attendee_path(attendee) %></td>
        <td><%= link_to 'Destroy', attendee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attendee', new_attendee_path %>
